    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/firebase": "^7.6.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.1.0",
    "ant-design-vue": "^1.7.7",
    "core-js": "^3.16.1",
    "crc": "^3.8.0",
    "firebase": "^8.9.1",
    "less": "^4.1.1",
    "less-loader": "6.2.0",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "slugify": "^1.6.0",
    "vue-clipboard2": "^0.3.1",
    "vue-markdown": "^2.2.4",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
